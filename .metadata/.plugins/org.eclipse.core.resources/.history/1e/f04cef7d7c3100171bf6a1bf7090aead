package com.language;

public class LanguageInfo {
	public static String L = "";
	
	public static void Select_Language (String LSelect){
		L = LSelect;
	}
	
	public static String See_Language (){
		return L;
	}
	
	public String Get_Time (){
		String Time;
		/* Get the current time depent of the country that is select*/
		Time = "10";
		
		
		return Time;
	}
	
	public String Get_Date (){
		String Date;
		/* Get the current time depent of the country that is select*/
		Date= "10";
		
		
		return Date;
	}
}
